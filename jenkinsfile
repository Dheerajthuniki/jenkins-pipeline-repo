pipeline {
     agent {label 'linux-node'}
     stages {
         stage('clean'){
             steps {
                 sh 'mvn clean'
             
             }
         }
         stage('compile') {
             steps {
                 sh 'mvn compile'
             }
         }
         stage('test'){
             steps {
                 sh 'mvn test'
             }
         }
         stage('install'){
             steps {
                 sh 'mvn install'
             }
         }
        
         
     }
      
 }
   
